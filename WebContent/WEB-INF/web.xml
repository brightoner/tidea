<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>student_history</display-name>
  <session-config>
    <session-timeout>180</session-timeout>
  </session-config>
  <filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>forceEncoding</param-name>
      <param-value>true</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>*.do</url-pattern>
  </filter-mapping>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>classpath*:egovframework/spring/context-*.xml</param-value>
  </context-param>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <servlet>
    <servlet-name>action</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/config/egovframework/springmvc/dispatcher-servlet.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>action</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/error/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500.jsp</location>
  </error-page>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <display-name>UbiGateway</display-name>
  <servlet>
    <servlet-name>UbiForm</servlet-name>
    <servlet-class>com.ubidcs.report.gateway.UbiForm</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UbiData</servlet-name>
    <servlet-class>com.ubidcs.report.gateway.UbiData</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UbiAdmin</servlet-name>
    <servlet-class>com.ubidcs.report.gateway.UbiAdmin</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UbiGateway</servlet-name>
    <servlet-class>com.ubidcs.report.gateway.UbiGateway</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UbiForm</servlet-name>
    <url-pattern>/UbiForm</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UbiData</servlet-name>
    <url-pattern>/UbiData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UbiAdmin</servlet-name>
    <url-pattern>/UbiAdmin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UbiGateway</servlet-name>
    <url-pattern>/UbiGateway</url-pattern>
  </servlet-mapping>
</web-app>